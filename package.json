{
  "name": "tumblelog-generator",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "start": "babel-node src/index.js",
    "test": "karma start karma/karma.config.development.js",
    "test:teamcity": "karma start karma/karma.config.production.js",
    "build:umd": "webpack src/index.js dist/tumblelog-generator.js --config webpack/webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/tumblelog-generator.min.js --config webpack/webpack.config.production.js",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min"
  },
  "npmName": "tumblelog-generator",
  "author": "Ian Delairre <idelairre@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idelairre/tumblelog-generator"
  },
  "bugs": {
    "url": "https://github.com/idelairre/tumblelog-generator/issues"
  },
  "keywords": [
    "tumblelog-generator"
  ],
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.x",
    "eslint": "^1.7.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "json5": "^0.4.0",
    "karma": "^0.13.9",
    "karma-htmlfile-reporter": "latest",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "latest",
    "karma-teamcity-reporter": "latest",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^2.1.3",
    "rimraf": "^2.3.4",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "es6-promise": "latest",
    "faker": "^3.1.0",
    "gingerbread": "^0.5.0",
    "lodash": "^4.2.1",
    "markovchain": "^1.0.2",
    "natural": "^0.4.0",
    "nlp-corpus": "0.0.4",
    "nlp-syllables": "0.0.5",
    "nlp_compromise": "^6.5.0",
    "pos": "^0.3.0",
    "random-words": "0.0.1",
    "raw-loader": "^0.5.1",
    "sentencer": "^0.1.5",
    "speller": "^1.0.1",
    "synaptic": "^1.0.8",
    "tensify": "0.0.2",
    "tumblr.js": "^1.1.1",
    "typo-js": "^1.0.1",
    "word2vec": "^1.1.0",
    "wordnet-db": "^3.1.2",
    "wordnetjs": "^0.3.0"
  },
  "xo": {
    "env": {
      "browser": true
    },
    "esnext": true,
    "rules": {
      "indent": [
        0
      ],
      "babel/object-curly-spacing": [
        0
      ],
      "babel/new-cap": [
        0
      ],
      "no-new": [
        0
      ],
      "camelcase": [
        0
      ],
      "xo/filename-case": [
        0
      ]
    }
  }
