{
  "name": "tumblr-faker",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test:node": "babel-node test/node.spec.js",
    "test": "karma start karma/karma.config.development.js",
    "test:teamcity": "karma start karma/karma.config.production.js",
    "build:umd": "webpack src/index.js --config webpack/webpack.config.development.js",
    "build:umd:min": "webpack src/index.js --config webpack/webpack.config.production.js",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min"
  },
  "npmName": "tumblr-faker",
  "author": "Ian Delairre <idelairre@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idelairre/tumblr-faker"
  },
  "bugs": {
    "url": "https://github.com/idelairre/tumblr-faker/issues"
  },
  "keywords": [
    "tumblelog-generator"
  ],
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^1.7.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^1.0.1",
    "karma-htmlfile-reporter": "latest",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "latest",
    "karma-teamcity-reporter": "latest",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^2.1.3",
    "rimraf": "^2.3.4",
    "tumblr.js": "^1.1.1",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "markovchain": "^1.0.2",
    "pos": "^0.3.0"
  },
  "xo": {
    "env": {
      "browser": true
    },
    "esnext": true,
    "rules": {
      "indent": [
        0
      ],
      "babel/object-curly-spacing": [
        0
      ],
      "babel/new-cap": [
        0
      ],
      "no-new": [
        0
      ],
      "camelcase": [
        0
      ],
      "xo/filename-case": [
        0
      ]
    }
  }
}
